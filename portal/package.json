{
  "name": "art",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "ng:upgrade": "ng update -- --all=true --force=true --next=true --verbose=true",
    "start": "ng serve --port=9001 --optimization=false --sourceMap=false",
    "serve": "cd dist && npx http-server --proxy \"http://localhost:8080?\"",
    "build": "ng build",
    "build:prod": "ng build -c=production",
    "nuclear": "rm -rf node_modules && rm package-lock.json && npm i",
    "_prettier": "prettier",
    "prettier": "npm run _prettier -- --write \"src/**/*.ts\"",
    "prettier:test": "npm run _prettier -- -l \"src/**/*.ts\"",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test:clear": "jest --clearCache",
    "test:unit": "jest --coverage --bail",
    "tw": "jest --watch",
    "//": "nr ng -- g @ngrx/schematics:feature feature/visual-regression/store/visual-regression -m feature/visual-regression/store/visual-regression-store.module.ts",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@angular/animations": "~10.0.2",
    "@angular/common": "~10.0.2",
    "@angular/compiler": "~10.0.2",
    "@angular/core": "~10.0.2",
    "@angular/forms": "~10.0.2",
    "@angular/platform-browser": "~10.0.2",
    "@angular/platform-browser-dynamic": "~10.0.2",
    "@angular/router": "~10.0.2",
    "@ngrx/effects": "^9.2.0",
    "@ngrx/router-store": "^9.2.0",
    "@ngrx/store": "^9.2.0",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^10.0.0-beta.0",
    "@angular-devkit/build-angular": "~0.1000.1",
    "@angular/cli": "~10.0.1",
    "@angular/compiler-cli": "~10.0.2",
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "@ngrx/schematics": "^9.2.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0-next.1",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "jest-preset-angular": "^8.2.1",
    "jest-sonar-reporter": "^2.0.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-scss": "^2.1.1",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "protractor": "~7.0.0",
    "tailwindcss": "^1.4.6",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
