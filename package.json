{
  "name": "emtrey",
  "version": "0.0.0",
  "scripts": {
    "start": "npm run build-ts && npm run serve",
    "serve:react": "DEFAULT_LIMIT=0 DRY_RUN=1 node dist/app.js --url=http://localhost:3000 --app=example-react",
    "serve": "DEFAULT_LIMIT=2 DRY_RUN=1 node dist/app.js -p=8080 --app=example-vue",
    "build": "rm -rf ./dist && npm run build-ts",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "build:watch": "tsc -w",
    "bw": "npm run build:watch"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "chrome-finder": "^1.0.7",
    "download-chromium": "^2.2.1",
    "glob": "^7.1.6",
    "handlebars": "^4.7.6",
    "prompts": "^2.4.1",
    "puppeteer-core": "^8.0.0",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@types/form-data": "^2.5.0",
    "@types/handlebars": "^4.1.0",
    "@types/node": "^14.0.14",
    "@types/prompts": "^2.0.10",
    "@types/puppeteer-core": "^5.4.0",
    "@types/rimraf": "^3.0.0",
    "prettier": "^2.0.5",
    "typescript": "^4.1.5"
  }
}
